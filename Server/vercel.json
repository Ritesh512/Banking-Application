{
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/build/(.*)",
        "dest": "/build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }